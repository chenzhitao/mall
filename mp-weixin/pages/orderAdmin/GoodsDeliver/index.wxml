<view class="deliver-goods"><view class="_header"><view class="order-num acea-row row-between-wrapper"><view class="num line1">{{"订单号："+delivery.orderId}}</view><view class="name line1">{{delivery.nickname}}</view></view><view class="address"><view class="name">{{''+delivery.realName+''}}<text class="phone">{{delivery.userPhone}}</text></view><view>{{delivery.userAddress}}</view></view><view class="line"><image src="{{$VUE_APP_RESOURCES_URL+'line.jpg'}}"></image></view></view><view class="wrapper"><view class="item acea-row row-between-wrapper"><view>发货方式</view><view class="mode acea-row row-middle row-right"><block wx:for="{{types}}" wx:for-item="item" wx:for-index="typesIndex" wx:key="typesIndex"><view data-event-opts="{{[['tap',[['changeType',['$0',typesIndex],[[['types','',typesIndex]]]]]]]}}" class="{{['goods',active===typesIndex?'on':'']}}" bindtap="__e">{{''+item.title+''}}<text class="iconfont icon-xuanzhong2"></text></view></block></view></view><view hidden="{{!(active===0)}}" class="list"><picker class="mode" mode="selector" range="{{logistics}}" range-key="name" data-event-opts="{{[['change',[['changeLog',['$event']]]]]}}" bindchange="__e"><view class="item acea-row row-between-wrapper"><view>快递公司</view><input class="mode" type="text" placeholder="填写快递公司" disabled="disabled" data-event-opts="{{[['input',[['__set_model',['','deliveryNames','$event',[]]]]]]}}" value="{{deliveryNames}}" bindinput="__e"/></view></picker><view class="item acea-row row-between-wrapper"><view>快递单号</view><input class="mode" type="text" placeholder="填写快递单号" data-event-opts="{{[['input',[['__set_model',['','deliveryId','$event',[]]]]]]}}" value="{{deliveryId}}" bindinput="__e"/></view></view><view hidden="{{!(active===1)}}" class="list"><view class="item acea-row row-between-wrapper"><view>送货人</view><input class="mode" type="text" placeholder="填写送货人" data-event-opts="{{[['input',[['__set_model',['','deliveryName','$event',[]]]]]]}}" value="{{deliveryName}}" bindinput="__e"/></view><view class="item acea-row row-between-wrapper"><view>送货电话</view><input class="mode" type="text" placeholder="填写送货电话" data-event-opts="{{[['input',[['__set_model',['','deliveryId','$event',[]]]]]]}}" value="{{deliveryId}}" bindinput="__e"/></view></view></view><view style="height:100rpx;"></view><view data-event-opts="{{[['tap',[['saveInfo',['$event']]]]]}}" class="confirm" bindtap="__e">确认提交</view></view>